<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef ParentName="AnimalThingBase" ADogSaidBody="LegsArmsTail">
    <defName>Cowgirl</defName>
    <label>cowgirl</label>
    <description>An animal which looks like a cow and human hybrid. It is very calm and can produce large amounts of milk.</description>
    <statBases>
      <MoveSpeed>4.8</MoveSpeed>
      <ComfyTemperatureMin>-14</ComfyTemperatureMin>
      <MarketValue>800</MarketValue>
      <ArmorRating_Blunt>0.2</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.2</ArmorRating_Sharp>
    </statBases>
    <comps>
      <li Class="CompProperties_Milkable">
        <milkDef>CowgirlMilk</milkDef>
        <milkIntervalDays>1</milkIntervalDays>
        <milkAmount>25</milkAmount>
      </li>
    </comps>
    <tools>
      <li>
        <label>teeth</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>8.8</power>
        <cooldownTime>2.6</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
        <chanceFactor>0.7</chanceFactor>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>7</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
      </li>
      <li>
        <label>left fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>7</power>
        <cooldownTime>1.6</cooldownTime>
        <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>22</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>right fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>7</power>
        <cooldownTime>1.6</cooldownTime>
        <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>22</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
    </tools>
    <race>
      <body>Cowgirl</body>
      <ageGenerationCurve>
        <points>
          <li>0.1,0</li>
          <li>0.5,10</li>
          <li>3,30</li>
          <li>15,50</li>
          <li>25,20</li>
          <li>26,0</li>
        </points>
      </ageGenerationCurve>
      <herdAnimal>true</herdAnimal>
      <baseBodySize>1</baseBodySize>
      <baseHungerRate>0.8</baseHungerRate>
      <baseHealthScale>1.25</baseHealthScale>
      <petness>0.6</petness>
      <wildness>0.05</wildness>
      <foodType>VegetarianRoughAnimal</foodType>
      <leatherDef>Leather_Cowgirl</leatherDef>
      <makesFootprints>true</makesFootprints>
      <lifeExpectancy>30</lifeExpectancy>
      <trainability>Advanced</trainability>
      <meatLabel>beef</meatLabel>
      <nuzzleMtbHours>12</nuzzleMtbHours>
      <herdMigrationAllowed>true</herdMigrationAllowed>
      <gestationPeriodDays>15</gestationPeriodDays>
      <litterSizeCurve>
